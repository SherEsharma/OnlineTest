<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>RegistrationController</servlet-name>
  	<display-name>RegistrationController</display-name>
  	<description></description>
  	<servlet-class>com.xenture.controller.RegistrationController</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>LoginController</servlet-name>
  	<display-name>LoginController</display-name>
  	<description></description>
  	<servlet-class>com.xenture.controller.LoginController</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>AdminLoginController</servlet-name>
  	<display-name>AdminLoginController</display-name>
  	<description></description>
  	<servlet-class>com.xenture.controller.AdminLoginController</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>CandidateLoginController</servlet-name>
  	<display-name>CandidateLoginController</display-name>
  	<description></description>
  	<servlet-class>com.xenture.controller.CandidateLoginController</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>ExamMainController</servlet-name>
  	<display-name>ExamMainController</display-name>
  	<description></description>
  	<servlet-class>com.xenture.controller.ExamMainController</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>AdminControllers</servlet-name>
  	<display-name>AdminControllers</display-name>
  	<description></description>
  	<servlet-class>com.xenture.controller.AdminControllers</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>RegistrationController</servlet-name>
  	<url-pattern>/RegistrationController</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>LoginController</servlet-name>
  	<url-pattern>/LoginController</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>AdminLoginController</servlet-name>
  	<url-pattern>/AdminLoginController</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>CandidateLoginController</servlet-name>
  	<url-pattern>/CandidateLoginController</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>ExamMainController</servlet-name>
  	<url-pattern>/ExamMainController</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>AdminControllers</servlet-name>
  	<url-pattern>/AdminControllers</url-pattern>
  </servlet-mapping>
  
  <welcome-file-list>
  <welcome-file>register.jsp</welcome-file>
  </welcome-file-list>

</web-app>
